{
  "name": "chatbiene",
  "version": "1.0.0",
  "description": "Realtime chat application with Vue and Socket.io",
  "main": "main.mjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "start:both": "concurrently \"npm run dev\" \"npm run serve\"",
    "electron:serve": "NODE_ENV=development electron .",
    "electron:build": "electron-builder"
  },
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-writer": "^1.6.0",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "moment": "^2.29.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "primevue": "^2.4.0",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "concurrently": "^6.5.1",
    "nodemon": "^2.0.6",
    "electron": "^25.3.1",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.example.chatbiene",
    "mac": {
      "target": "dmg"
    },
    "files": [
      "dist/**/*",
      "main.mjs",
      "preload.js"
